<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.antonioreyes.aguas" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <!--
 The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but are recommended.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".formulario_reporte"
            android:label="@string/title_activity_formulario_reporte" >
        </activity>
        <activity
            android:name=".MapsActivity2"
            android:label="@string/title_activity_maps_activity2" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name=".ContaminacionAgua"
            android:label="@string/title_activity_contaminacion_agua" >
        </activity>
        <activity
            android:name=".deslaves"
            android:label="@string/title_activity_deslaves" >
        </activity>
        <activity
            android:name=".Encharcamientos"
            android:label="@string/title_activity_encharcamientos" >
        </activity>
        <activity
            android:name=".FaltaAgua"
            android:label="@string/title_activity_falta_agua" >
        </activity>
        <activity
            android:name=".FugaAgua"
            android:label="@string/title_activity_fuga_agua" >
        </activity>
        <activity
            android:name=".Hundimientos"
            android:label="@string/title_activity_hundimientos" >
        </activity>
        <activity
            android:name=".Infraestructura"
            android:label="@string/title_activity_infraestructura" >
        </activity>
        <activity
            android:name=".Inundaciones"
            android:label="@string/title_activity_inundaciones" >
        </activity>
        <activity
            android:name=".Deslaves"
            android:label="@string/title_activity_deslaves" >
        </activity>
    </application>

</manifest>
